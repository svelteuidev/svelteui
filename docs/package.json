		"autoprefixer": "10.4.4",
		"cssnano": "5.1.7",
		"svelte-adapter-github": "^0.0.1-next.0",
		"svelte2tsx": "0.5.7"
	},
	"type": "module",
	"dependencies": {
		"@svelteuidev/actions": "latest",
		"@svelteuidev/core": "latest",
		"@svelteuidev/motion": "latest",
		"@svelteuidev/utilities": "latest",
		"radix-icons-svelte": "^1.2.1"
	}
}
