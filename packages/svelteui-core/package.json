{
	"name": "@svelteuidev/core",
	"version": "0.11.1",
	"description": "Svelte component library for creating fully functional and accessible web applications quickly",
	"keywords": [
		"svelte",
		"sveltekit",
		"svelteui",
		"ui",
		"components",
		"ui-kit",
		"library",
		"frontend",
		"design",
		"stitches"
	],
	"homepage": "https://svelteui.org",
	"repository": {
		"type": "git",
		"url": "https://github.com/svelteuidev/svelteui",
		"directory": "packages/svelteui-core"
	},
	"license": "MIT",
	"author": "Kamell Perry <kamellperry33@gmail.com>",
	"sideEffects": false,
	"type": "module",
	"module": "./package/index.js",
	"types": "types/index.d.ts",
	"files": [
		"index.js",
		"stitches.config.js",
		"components/**/*.{js,svelte}",
		"internal/**/*.{js,svelte}",
		"styles/**/*.{js,svelte}",
		"types/**/*.d.ts"
	],
	"scripts": {
		"build": "vite build",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "vite dev",
		"format": "prettier --write --plugin-search-dir=. src/",
		"generate-types": "deno run --unstable --allow-read --allow-write ../../scripts/generate-types.ts",
		"lint": "prettier --check --plugin-search-dir=. src/ && eslint .",
		"package": "svelte-package",
		"prepare": "svelte-kit sync",
		"preview": "vite preview",
		"pub": "cd package && yarn publish",
		"pub:test": "npm publish package/",
		"sort": "npx sort-package-json",
		"test": "vitest --run",
		"test:watch": "vitest",
		"update:lockfile": "yarn",
		"watch": "svelte-package -w"
	},
	"dependencies": {
		"@floating-ui/dom": "0.5.2",
		"@stitches/core": "1.2.8"
	},
	"devDependencies": {
		"@babel/core": "7.18.6",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@sveltejs/package": "1.0.0-next.3",
		"@typescript-eslint/eslint-plugin": "5.27.0",
		"@typescript-eslint/parser": "5.27.0",
		"autoprefixer": "10.4.4",
		"babel-loader": "8.2.5",
		"cssnano": "5.1.7",
		"eslint": "8.16.0",
		"eslint-config-prettier": "8.3.0",
		"eslint-plugin-storybook": "0.6.1",
		"eslint-plugin-svelte3": "4.0.0",
		"postcss": "8.4.12",
		"prettier": "2.6.2",
		"prettier-plugin-svelte": "2.7.0",
		"sass": "1.50.0",
		"storybook-dark-mode": "1.1.0",
		"svelte": "3.55.0",
		"svelte-check": "3.0.2",
		"svelte-loader": "3.1.3",
		"svelte-preprocess": "4.10.6",
		"svelte2tsx": "0.5.7",
		"tslib": "2.3.1",
		"typescript": "4.7.4",
		"vite": "3.2.2"
	},
	"peerDependencies": {
		"@svelteuidev/composables": "0.11.1",
		"svelte": ">=3.55.0"
	}
}