{
	"name": "@svelteuidev/core",
	"version": "0.15.7",
	"description": "Svelte component library for creating fully functional and accessible web applications quickly",
	"keywords": [
		"svelte",
		"sveltekit",
		"svelteui",
		"ui",
		"components",
		"ui-kit",
		"library",
		"frontend",
		"design",
		"stitches"
	],
	"homepage": "https://svelteui.dev",
	"repository": {
		"type": "git",
		"url": "https://github.com/svelteuidev/svelteui",
		"directory": "packages/svelteui-core"
	},
	"license": "MIT",
	"author": "Kamell Perry <kamellperry33@gmail.com>",
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"svelte": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"typesVersions": {
		">4.0": {
			"*": [
				"./dist/index.d.ts"
			]
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "vite build",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "vite dev",
		"format": "prettier --write --plugin-search-dir=. .",
		"lint": "prettier --check --plugin-search-dir=. src/ && eslint .",
		"package": "svelte-kit sync && svelte-package",
		"preview": "vite preview",
		"pub": "yarn publish",
		"pub:test": "npm publish dist/",
		"sort": "npx sort-package-json",
		"sync": "svelte-kit sync",
		"test": "vitest --run",
		"test:watch": "vitest",
		"update:lockfile": "yarn",
		"watch": "svelte-package -w"
	},
	"dependencies": {
		"@floating-ui/dom": "1.2.8",
		"@stitches/core": "1.2.8"
	},
	"devDependencies": {
		"@babel/core": "7.21.8",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.5.27",
		"@sveltejs/package": "^2.3.7",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@testing-library/svelte": "5.2.6",
		"autoprefixer": "10.4.14",
		"babel-loader": "9.1.2",
		"cssnano": "6.0.1",
		"eslint-plugin-storybook": "^0.11.2",
		"postcss": "8.4.23",
		"prettier": "^3.1.0",
		"prettier-plugin-svelte": "^3.2.6",
		"sass": "1.62.1",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-loader": "^3.2.4",
		"svelte-preprocess": "^6.0.0",
		"svelte2tsx": "0.6.14",
		"tslib": "2.5.0",
		"typescript": "^5.5.0",
		"vite": "^5.4.4",
		"@eslint/compat": "^1.2.3",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"typescript-eslint": "^8.0.0"
	},
	"peerDependencies": {
		"@svelteuidev/composables": "0.15.7",
		"svelte": ">=5.0.0"
	}
}
