{
	"name": "@svelteuidev/actions",
	"version": "0.5.5",
	"type": "module",
	"scripts": {
		"build:major": "npm --no-git-tag-version version major && npm run package && cd package && npm publish",
		"build:minor": "npm --no-git-tag-version version minor && npm run package && cd package && npm publish",
		"build:patch": "npm --no-git-tag-version version patch && npm run package && cd package && npm publish",
		"build:patch:nopub": "npm --no-git-tag-version version patch && npm run package",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"coverage": "vitest run --coverage",
		"dev": "svelte-kit dev",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"package": "svelte-kit package",
		"sort": "npx sort-package-json",
		"test": "vitest --run",
		"test:watch": "vitest"
	},
	"devDependencies": {
		"@sveltejs/kit": "1.0.0-next.310",
		"@svelteuidev/tsconfig": "*",
		"@testing-library/svelte": "3.1.1",
		"autoprefixer": "10.4.4",
		"cssnano": "5.1.7",
		"eslint-preset-svelteui": "*",
		"jsdom": "19.0.0",
		"postcss": "8.4.12",
		"sass": "1.50.0",
		"svelte": "3.47.0",
		"svelte-check": "2.5.0",
		"svelte-preprocess": "4.10.5",
		"svelte2tsx": "0.5.7",
		"tslib": "2.3.1",
		"typescript": "4.6.3"
	}
}
